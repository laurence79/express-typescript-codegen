{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "BadRequest": {
      "type": "object",
      "required": [
        "errors"
      ],
      "properties": {
        "errors": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "TokenResponse": {
      "type": "object",
      "required": [
        "access_token",
        "expires_in"
      ],
      "properties": {
        "access_token": {
          "type": "string"
        },
        "expires_in": {
          "type": "integer"
        }
      }
    },
    "GrantResponse": {
      "type": "object",
      "required": [
        "id",
        "userId"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "userId": {
          "type": "string"
        },
        "createDateTime": {
          "type": "string"
        },
        "grantedByUserId": {
          "type": "string"
        },
        "global": {
          "type": "object",
          "required": [
            "role"
          ],
          "properties": {
            "role": {
              "type": "string",
              "enum": [
                "ADMIN"
              ]
            }
          }
        },
        "venue": {
          "type": "object",
          "required": [
            "id",
            "role"
          ],
          "properties": {
            "id": {
              "type": "string"
            },
            "role": {
              "type": "string",
              "enum": [
                "MANAGER"
              ]
            }
          }
        }
      }
    },
    "GrantRequest": {
      "type": "object",
      "required": [
        "userId"
      ],
      "properties": {
        "userId": {
          "type": "string"
        },
        "global": {
          "type": "object",
          "required": [
            "role"
          ],
          "properties": {
            "role": {
              "type": "string",
              "enum": [
                "ADMIN"
              ]
            }
          }
        },
        "venue": {
          "type": "object",
          "required": [
            "id",
            "role"
          ],
          "properties": {
            "id": {
              "type": "string"
            },
            "role": {
              "type": "string",
              "enum": [
                "MANAGER"
              ]
            }
          }
        }
      }
    },
    "GetHealth200ResponseBody": {
      "type": "object",
      "properties": {
        "ok": {
          "type": "boolean"
        }
      }
    },
    "PostAnonymousRequestBody": {
      "type": "object",
      "required": [
        "seed"
      ],
      "properties": {
        "seed": {
          "type": "string",
          "description": "A unique ID for the anonymous user"
        }
      }
    },
    "PostAnonymous200ResponseBody": {
      "$ref": "#/definitions/TokenResponse"
    },
    "PostAnonymous400ResponseBody": {
      "$ref": "#/definitions/BadRequest"
    },
    "PostCreateRequestBody": {
      "type": "object",
      "required": [
        "emailAddress",
        "password",
        "displayName"
      ],
      "properties": {
        "emailAddress": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "displayName": {
          "type": "string"
        },
        "photoUrl": {
          "type": "string"
        }
      }
    },
    "PostCreate200ResponseBody": {
      "type": "object",
      "required": [
        "uid"
      ],
      "properties": {
        "uid": {
          "type": "string"
        }
      }
    },
    "PostCreate400ResponseBody": {
      "$ref": "#/definitions/BadRequest"
    },
    "PostLoginRequestBody": {
      "type": "object",
      "required": [
        "emailAddress",
        "password"
      ],
      "properties": {
        "emailAddress": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      }
    },
    "PostLogin200ResponseBody": {
      "$ref": "#/definitions/TokenResponse"
    },
    "PostLogin400ResponseBody": {
      "$ref": "#/definitions/BadRequest"
    },
    "PostVerify200ResponseBody": {
      "$ref": "#/definitions/TokenResponse"
    },
    "GetGrants200ResponseBody": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/GrantResponse"
      }
    },
    "GetGrants400ResponseBody": {
      "$ref": "#/definitions/BadRequest"
    },
    "PostGrantsRequestBody": {
      "$ref": "#/definitions/GrantRequest"
    },
    "PostGrants200ResponseBody": {
      "$ref": "#/definitions/GrantResponse"
    },
    "PostGrants400ResponseBody": {
      "$ref": "#/definitions/BadRequest"
    },
    "GetGrantById200ResponseBody": {
      "$ref": "#/definitions/GrantResponse"
    },
    "GetGrantById400ResponseBody": {
      "$ref": "#/definitions/BadRequest"
    },
    "DeleteGrantById400ResponseBody": {
      "$ref": "#/definitions/BadRequest"
    }
  }
}